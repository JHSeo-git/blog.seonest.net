{
  "name": "blog.seonest.net",
  "version": "0.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JHSeo-git/blog.seonest.net.git"
  },
  "author": {
    "name": "JHSeo",
    "email": "qnfqnfqnf@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "next build",
    "dev": "next dev",
    "format": "eslint --fix --ext .ts,.tsx,.js,.jsx . && prettier --write 'src/**/*.{js,jsx,ts,tsx}' --cache",
    "postinstall": "npx prisma generate",
    "lint": "next lint",
    "post": "seonest-post-gen -d content/posts",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "tz": "date -u +\"%Y-%m-%dT%H:%M:%SZ\""
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.13.5",
    "@giscus/react": "^3.0.0",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toggle": "^1.0.3",
    "@vercel/analytics": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.4.0",
    "framer-motion": "^11.0.12",
    "giscus": "^1.5.0",
    "github-slugger": "^2.0.0",
    "lucide-react": "^0.356.0",
    "next": "14.1.3",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-medium-image-zoom": "^5.1.10",
    "react-tweet": "^3.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "3.0.1",
    "sharp": "^0.33.2",
    "shiki": "^1.1.7",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@next/bundle-analyzer": "^14.1.3",
    "@types/glob": "^8.1.0",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "prisma": "^5.11.0",
    "seonest-post-gen": "^0.2.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-radix": "^2.8.0",
    "typescript": "^5.4.2"
  },
  "pnpm": {
    "overrides": {
      "@contentlayer/core>esbuild": "$esbuild",
      "next-contentlayer>next": "$next"
    }
  },
  "prisma": {
    "seed": "node -r esbuild-register prisma/seed.ts"
  }
}
