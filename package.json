    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-is": "19.2.3",
    "react-medium-image-zoom": "^5.4.0",
    "react-twc": "^1.5.1",
    "react-tweet": "^3.3.0",
    "sharp": "^0.34.5",
    "shiki": "^3.20.0",
