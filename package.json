    "eslint-plugin-tailwindcss": "^3.10.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.11.0",
    "seonest-post-gen": "^0.1.2",
    "tailwindcss": "^3.2.7",
