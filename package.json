  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "pnpm build:content && next build --turbopack",
    "build:content": "contentlayer2 build",
    "dev": "next dev --turbopack",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\" --cache",
    "postinstall": "npx prisma generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "post": "seonest-post-gen -d content/posts",
    "prepare": "simple-git-hooks",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "tz": "date -u +\"%Y-%m-%dT%H:%M:%SZ\""
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@giscus/react": "^3.1.0",
