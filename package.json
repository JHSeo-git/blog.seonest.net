    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-radix": "^4.0.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.2.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  },
  "prisma": {
    "seed": "node -r esbuild-register prisma/seed.ts"
  }
